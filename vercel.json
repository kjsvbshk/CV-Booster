{
  "version": 2,
  "name": "cv-booster-frontend",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "/api",
    "VITE_NODE_ENV": "production"
  }
}
